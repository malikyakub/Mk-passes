{
  "expo": {
    "name": "Mk-passes",
    "slug": "Mk-passes",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icons/bg-removed-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/light-logo.png",
        "backgroundColor": "#0A97B0"
      },
      "package": "com.malikyakub.Mkpasses",
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/app-icons/bg-removed-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/light-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "packagerOpts": {
      "sourceExts": [
        "js",
        "jsx",
        "ts",
        "tsx",
        "svg"
      ]
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2a32fa31-5d87-4d1a-be43-3a5013160922"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/2a32fa31-5d87-4d1a-be43-3a5013160922"
    }
  }
}
